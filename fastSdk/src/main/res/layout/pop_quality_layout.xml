<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/pop_quality_bg"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_sd"
        style="@style/pop_more_item_ly">

        <TextView
            android:id="@+id/tv_sd"
            style="@style/pop_quality_style_tv"
            android:text="@string/quality_sd" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_hd"
        style="@style/pop_more_item_ly">

        <TextView
            android:id="@+id/tv_hd"
            style="@style/pop_quality_style_tv"
            android:text="@string/quality_hd" />

    </LinearLayout>


</LinearLayout>